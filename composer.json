{
    "name": "jopawu/contao-culture-events-bundle",
    "description": "Manage and present culture events with this extension.",
    "keywords": [
        "contao",
        "bundle",
        "tag 3",
        "tag 4"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Bj√∂rn Heyser",
            "email": "bh@bjoernheyser.de",
            "homepage": "https://github.com/bheyser",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/jopawu/contao-culture-events-bundle/issues",
        "source": "https://github.com/jopawu/contao-culture-events-bundle"
    },
    "require": {
        "php": "^7.4",
        "contao/core-bundle": "^4.13 || ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Jopawu\\ContaoCultureEventsBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": false,
            "contao/manager-plugin": false,
            "contao-community-alliance/composer-plugin": true
        }
    },
    "extra": {
        "contao-manager-plugin": "Jopawu\\ContaoCultureEventsBundle\\ContaoManager\\Plugin"
    },
    "scripts": {
        "cs-fixer": "@php tools/ecs/vendor/bin/ecs check config/ contao/ src/ templates/ tests/ --config tools/ecs/config.php --fix --ansi",
        "unit-tests": "@php tools/phpunit/vendor/bin/phpunit -c tools/phpunit/phpunit.xml.dist"
    },
    "version": "1.0"
}
